{
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^8.7.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "4.3.8",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  },
  "scripts": {
    "cy:run:heroku": "npx cypress run --env configFile=heroku",
    "cy:open:heroku": "npx cypress open --env configFile=heroku",
    "cy:prettier": "prettier --write \"cypress/**/*.js\"",
    "cy:lint": "eslint cypress/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {}
}
